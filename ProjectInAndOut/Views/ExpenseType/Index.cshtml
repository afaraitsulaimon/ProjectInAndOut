@model IEnumerable<ProjectInAndOut.Models.ExpenseType>

 <div class="container p-3">
<div class="row pt-4">
    <div class="col-6">
        <h2 class="text-primary">
            Expenses Type List
        </h2>
</div>
    <div class="col-6 text-right">
            <a asp-area="" asp-controller="ExpenseType" asp-action="Create" class="btn btn-primary">Create New Expense Type</a>
</div>
</div>
<br/>

    @if(Model.Count() > 0){
        <table class="table table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th>Name</th>
                     <th>Changes</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
               {
                       <tr>
                        <td style="width:35%">@obj.Name</td>
                         <td style="width:30%">
                             <div class="w-100 btn-group" role="group">
                                 <a asp-area="" asp-controller="ExpenseType" asp-action="Update" asp-route-Id="@obj.Id" class="btn btn-primary mx-1">Update</a>
                                 <a asp-area="" asp-controller="ExpenseType" asp-action="Delete" asp-route-Id="@obj.Id" class="btn btn-danger mx-1">Delete</a>

                             </div>

                         </td>

                    </tr>
                   
               }
            </tbody>
        </table>
    }else{
                   <h1>No Item created yet</h1>
               }
</div>